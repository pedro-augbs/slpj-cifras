generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Music {
  id     Int    @id @default(autoincrement())
  name   String
  slug   String @unique
  artist String
  author String
  bpm    Int
  letter String
  key    String
}
